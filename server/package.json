{
  "name": "tabak-taste-api",
  "version": "1.0.0",
  "description": "RESTful API for tabak-taste.de",
  "main": "api.js",
  "scripts": {
    "build": "tsc",
    "start": "cross-env NODE_ENV=production node dist/src/api.js",
    "dev": "cross-env NODE_ENV=development nodemon src/api.ts",
    "db:update": "prisma db push",
    "docker-up:dev": "docker compose -f docker-compose.dev.yml up -d --wait",
    "docker-down:dev": "docker compose -f docker-compose.dev.yml down"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ikke09/Tabak-Taste.git"
  },
  "keywords": [
    "Rest",
    "REST",
    "API",
    "Tabak",
    "Tobacco",
    "Taste",
    "Geschmack",
    "Shisha"
  ],
  "author": "Christian Bechtluft (ikke09)",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/ikke09/Tabak-Taste/issues"
  },
  "homepage": "https://github.com/ikke09/Tabak-Taste#readme",
  "dependencies": {
    "@prisma/client": "^5.3.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "mongoose": "^7.5.3",
    "swagger-ui-express": "^5.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.18",
    "@types/node": "^20.8.2",
    "cross-env": "^7.0.3",
    "nodemon": "^3.0.1",
    "prisma": "^5.4.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "engines": {
    "npm": ">= 7.0.0",
    "yarn": "Please use npm instead of yarn.",
    "pnpm": "Please use npm instead of pnpm."
  }
}