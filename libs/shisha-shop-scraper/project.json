{
  "name": "shisha-shop-scraper",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/shisha-shop-scraper/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/libs/shisha-shop-scraper",
        "main": "libs/shisha-shop-scraper/src/index.ts",
        "tsConfig": "libs/shisha-shop-scraper/tsconfig.lib.json",
        "assets": [
          "libs/shisha-shop-scraper/*.md"
        ]
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": [
        "{options.outputFile}"
      ]
    },
    "test": {
      "executor": "@nx/vite:test",
      "outputs": [
        "{options.reportsDirectory}"
      ],
      "options": {
        "reportsDirectory": "../../coverage/libs/shisha-shop-scraper"
      }
    }
  },
  "tags": [
    "type:lib"
  ]
}